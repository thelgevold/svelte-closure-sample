{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run closure",
    "dev": "rollup -c -w",
    "start": "npm run build && node server.js",
    "compile": "node compile.js",
    "closure": "npm run compile && java -jar node_modules/google-closure-compiler-java/compiler.jar --flagfile closure.conf",
    "closure-debug": "java -jar node_modules/google-closure-compiler-java/compiler.jar --flagfile closure.conf --debug --formatting PRETTY_PRINT"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "3.30.0",
    "google-closure-compiler": "20201102.0.1",
    "tslib": "2.0.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "express": "4.17.1",
    "compression": "1.7.4",
    "rxjs": "6.4.0",
    "page": "1.11.6",
    "svelte-forms-lib": "1.2.2"
  }
}